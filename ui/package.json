    "cypress": "cypress run --headless --browser chrome",
    "format": "lerna run format",
    "lint-staged": "lerna run lint-staged",
    "unit:test": "lerna run test",
    "i18n": "lerna run i18n --scope=@debezium/ui"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "lint-staged": {
