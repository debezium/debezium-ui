  "private": true,
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "scripts": {
    "lint": "lerna run --parallel --no-bail lint",
    "clean": "lerna run clean",
    "build": "lerna run build",
    "start": "lerna run start --scope=@debezium/ui",
    "start:federate": "lerna run start:federate --scope=@debezium/ui",
    "start:prod": "lerna run start:prod --scope=@debezium/ui",
